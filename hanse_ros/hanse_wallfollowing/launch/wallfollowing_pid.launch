<launch>

  <!-- WALLFOLLOWING -->

  <node name="wallfollowing_pid" pkg="hanse_wallfollowing" type="wallfollowing_pid.py" output="screen" />

  <!-- ANGULAR PID -->

  <group ns="wallfollowing_angular_pid">
    <rosparam file="$(find hanse_wallfollowing)/config_angular_pid.yaml"/>
    <node name="pid" pkg="hanse_pidcontrol" type="pid_control" output="screen"></node>
  </group>
</launch>
